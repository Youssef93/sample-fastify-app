{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "modulePaths": [
    "<rootDir>"
  ],
  "testRegex": ".*\\.spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": [
    "**/*.ts"
  ],
  "coverageDirectory": "../coverage",
  "collectCoverage": true,
  "coveragePathIgnorePatterns": [
    "main.ts"
  ],
  "testEnvironment": "node"
}